int fib(int x) {
  int y = if (x > 2) fib(x-1) + fib(x-2) else 1;
  return y;
}

prints("6th fibonacci number:");
printi(fib(6));

struct Math {
  int a;
  float b;
  char[] verb;
  (int, float)->(float) op;
};

struct Math m = {4, 3.14, "times", (int x, float y)->{ return x * y; }}; /* Anonymous functions */
struct Math d = {10, 3.5, "divided by", (int x, float y)->{ return x / y; }};

void eval(struct Math x) {
  prints(" ");
  printi(x.a);
  prints(x.verb);
  printf(x.b);
  prints("equals");
  printf((x.op)(x.a, x.b));
  return;
}

eval(m);
eval(d);
