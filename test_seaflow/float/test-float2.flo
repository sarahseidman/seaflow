void testFloat(float a, float b) {
    printf(a + b);
    printf(a - b);
    printf(a * b);
    printf(a / b);
    printi(if(a == b) 1 else 0);
    printi(if(a == a) 1 else 0);
    printi(if(a != b) 1 else 0);
    printi(if(a != a) 1 else 0);
    printi(if(a > b) 1 else 0);
    printi(if(a >= b) 1 else 0);
    printi(if(a < b) 1 else 0);
    printi(if(a <= b) 1 else 0);
}

void foo() {
  float c = 42.0;
  float d = 3.14159;

  testFloat(c, d);
  testFloat(d, d);
}

foo();
