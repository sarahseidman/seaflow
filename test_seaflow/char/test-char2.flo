void testChar(char a, char b) {
    printf(a + b);
    printf(a - b);
    printf(a * b);
    printf(a / b);
    printi(if(a == b) 1 else 0);
    printi(if(a == a) 1 else 0);
    printi(if(a != b) 1 else 0);
    printi(if(a != a) 1 else 0);
    printi(if(a > b) 1 else 0);
    printi(if(a >= b) 1 else 0);
    printi(if(a < b) 1 else 0);
    printi(if(a <= b) 1 else 0);
}

void foo() {
  char c = 'h';
  char d = 'e';

  testChar(c, d);
  testChar(d, d);
}

foo();
